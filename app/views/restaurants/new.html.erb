<div class="container">

  <h2>Add New Restaurant</h2>
  <h4> <%=simple_form_for(@restaurant) do |f| %>
   <%= f.label :name %>
   <%= f.text_field :name %>

   <%= f.label :address %>
   <%= f.text_field :address %>

   <%= f.label :category %>
   <%= f.text_field :category %>

   <%= f.label :phone_number %>
   <%= f.text_field :phone_number %>

   <%= f.button :submit %>
   <% end %>
  </h4>
</div>


